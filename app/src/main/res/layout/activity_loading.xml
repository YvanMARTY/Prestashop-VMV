<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.vmvlimayrac.app.vmv.LoadingActivity">



    <ImageView android:layout_width="140sp" android:layout_height="100sp"

               android:layout_marginBottom="8dp"
               android:background="@drawable/logotexte"
               app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
               app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
               android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
               android:layout_marginStart="8dp" android:id="@+id/imageView"/>
    <ProgressBar
            style="?android:attr/progressBarStyle"
            android:layout_width="272dp"
            android:layout_height="254dp"
            android:id="@+id/progressBar"
            app:layout_constraintTop_toTopOf="@+id/imageView" app:layout_constraintBottom_toBottomOf="@+id/imageView"
            app:layout_constraintStart_toStartOf="@+id/imageView" app:layout_constraintEnd_toEndOf="@+id/imageView"/>

</android.support.constraint.ConstraintLayout>
